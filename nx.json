{
  "npmScope": "ngfire-showcase",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "coljobs": {
      "tags": []
    },
    "coljobs-e2e": {
      "tags": [],
      "implicitDependencies": ["coljobs"]
    },
    "shared-ui-forms": {
      "tags": ["scope:shared", "type:ui"]
    },
    "shared-data-access-firebase": {
      "tags": ["scope:shared", "type:data-access"]
    },
    "shared-environments": {
      "tags": ["scope:shared", "type:environments"]
    }
  }
}
